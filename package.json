{
  "name": "auth0-cloudflare-nextjs",
  "version": "1.0.9",
  "description": "Auth0 authentication for Next.js on Cloudflare Workers",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./client": {
      "import": "./dist/client/index.mjs",
      "require": "./dist/client/index.js",
      "types": "./dist/client/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/client/index.tsx --format cjs,esm --dts --external react",
    "lint": "eslint src/**/*.ts",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "prepublishOnly": "npm run build",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ShiresFam/auth0-cloudflare-nextjs.git"
  },
  "bugs": {
    "url": "https://github.com/ShiresFam/auth0-cloudflare-nextjs/issues"
  },
  "homepage": "https://github.com/ShiresFam/auth0-cloudflare-nextjs#readme",
  "keywords": [
    "auth0",
    "cloudflare",
    "workers",
    "nextjs",
    "authentication",
    "auth",
    "oauth",
    "openid",
    "typescript"
  ],
  "author": "Trevor Shires <trevor@shiresfam.com>",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  },
  "peerDependencies": {
    "@opennextjs/cloudflare": "^0.5.11",
    "next": "^14.0.0 || ^15.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250313.0",
    "@opennextjs/cloudflare": "^0.5.11",
    "@types/node": "^20.4.2",
    "@types/react": "^19.0.7",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.44.0",
    "force": "^0.0.3",
    "tsup": "^7.1.0",
    "typescript": "^5.1.6",
    "wrangler": "^4.0.0"
  },
  "dependencies": {
    "jwt-decode": "^4.0.0"
  }
}