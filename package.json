{
  "name": "auth0-cloudflare-nextjs",
  "version": "1.0.0",
  "description": "Auth0 authentication for Next.js on Cloudflare Workers",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./client": {
      "import": "./dist/client.mjs",
      "require": "./dist/client.js",
      "types": "./dist/client.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/client.tsx --format cjs,esm --dts --external react",
    "lint": "eslint src/**/*.ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "auth0",
    "cloudflare",
    "workers",
    "nextjs",
    "authentication"
  ],
  "author": "Your Name",
  "license": "MIT",
  "peerDependencies": {
    "@opennextjs/cloudflare": "^0.3.5",
    "next": "^14.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@opennextjs/cloudflare": "^0.3.5",
    "@types/node": "^20.4.2",
    "@types/react": "^19.0.7",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.44.0",
    "tsup": "^7.1.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "jwt-decode": "^4.0.0"
  }
}
